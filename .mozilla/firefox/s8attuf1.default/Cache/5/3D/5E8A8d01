                        </m:mtd>
                        <m:mtd>
                          <m:mrow>
                            <m:mrow/>
                            <m:mrow>
                              <m:mo stretchy="false">∣</m:mo>
                              <m:mtable>
                                <m:mtr>
                                  <m:mtd>
                                    <m:mrow>
                                      <m:msub>
                                        <m:mi>a</m:mi>
                                        <m:mstyle fontsize="8pt">
                                          <m:mrow>
                                            <m:mtext>11</m:mtext>
                                          </m:mrow>
                                        </m:mstyle>
                                      </m:msub>
                                      <m:mrow/>
                                    </m:mrow>
                                  </m:mtd>
                                  <m:mtd>
                                    <m:mrow>
                                      <m:msub>
                                        <m:mi>a</m:mi>
                                        <m:mstyle fontsize="8pt">
                                          <m:mrow>
                                            <m:mtext>12</m:mtext>
                                          </m:mrow>
                                        </m:mstyle>
                                      </m:msub>
                                      <m:mrow/>
