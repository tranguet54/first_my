                        adcontainerHeight = 200;
                    $('#adcontainer').height(adcontainerHeight);

                    var top = $('#advs').offset().top - parseFloat($('#advs').css('margin-top').replace(/auto/, 0));
                    $(window).scroll(function (event) {
                        // what the y position of the scroll is
                        var y = $(this).scrollTop();

                        // whether that's below the form
                        if (y >= top && y <= $('#content').height()) {
                            // if so, ad the fixed class
                            $('#advs').addClass('fixed');
                        } else {
                            // otherwise remove it
                            $('#advs').removeClass('fixed');
                        }
                    });
                }
            });
        </script>
