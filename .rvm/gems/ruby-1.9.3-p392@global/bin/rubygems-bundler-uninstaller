require 'rubygems'

version = ">= 0"

if ARGV.first =~ /^_(.*)_$/ and Gem::Version.correct? $1 then
  version = $1
  ARGV.shift
end

gem 'rubygems-bundler', version
load Gem.bin_path('rubygems-bundler', 'rubygems-bundler-uninstaller', version)
