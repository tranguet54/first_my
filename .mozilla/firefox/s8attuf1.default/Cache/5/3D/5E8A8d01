A {} # = lline  matrix {
a rSub { size 8{"11"} }  {} # a rSub { size 8{"12"} }  {} #  "."  "."  "."  {} # a rSub { size 8{1n} }  {} ##
a rSub { size 8{"21"} }  {} # a rSub { size 8{"22"} }  {} #  "."  "."  "."  {} # a rSub { size 8{2n} }  {} ##
 "."  "."  "."  {} #  "."  "."  "."  {} #  "."  "."  "."  {} #  "."  "."  "."  {} ##
a rSub { size 8{m1} }  {} # a rSub { size 8{m2} }  {} #  "."  "."  "."  {} # a rSub { size 8{ ital "mn"} } {}
}  rline {}
} = left [a rSub { size 8{i`j} }  right ]} {}</m:annotation>
